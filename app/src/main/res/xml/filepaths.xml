<?xml version="1.0" encoding="utf-8"?>
<paths>
    <!--1、对应内部内存卡根目录：Context.getFileDir()-->
    <files-path
        name="files_path"
        path="/" />

    <!--2、对应应用默认缓存根目录：Context.getCacheDir()-->
    <cache-path
        name="cache_path"
        path="/" />

    <!--3、对应外部内存卡根目录：Environment.getExternalStorageDirectory()-->
    <external-path
        name="external_path"
        path="/" />

    <!--4、对应外部内存卡根目录下的APP公共目录：Context.getExternalFileDir(String)-->
    <external-files-path
        name="external_files_path"
        path="/" />

    <!--5、对应外部内存卡根目录下的APP缓存目录：Context.getExternalCacheDir()-->
    <external-cache-path
        name="external_cache_path"
        path="/" />

    <external-media-path
        name="external_media_path"
        path="/" />
    <external-files-path
        name="umeng_cache"
        path="umeng_cache/" />
    <!-- QQ 官方分享SDK 共享路径 -->
    <root-path
        name="opensdk_root"
        path="" />
    <external-files-path
        name="opensdk_external"
        path="Images/tmp" />
    <root-path
        name="opensdk_root"
        path="" />
</paths>